{
  "name": "react-list-separator",
  "version": "0.0.10",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "build": "tsc && vite build",
    "check": "biome check src",
    "check:fix": "biome check --write src",
    "type:check": "tsc --noEmit",
    "release": "npm run build && npm publish"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/react-list-separator.cjs.js",
  "module": "./dist/react-list-separator.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/react-list-separator.es.js",
      "require": "./dist/react-list-separator.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "react",
    "separator",
    "list",
    "component"
  ],
  "author": "tommykw",
  "license": "MIT",
  "description": "A React utility library for adding customizable separators between list items.",
  "peerDependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.2",
    "@testing-library/jest-dom": "6.5.0",
    "@testing-library/react": "16.0.0",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/jsdom": "21.1.7",
    "typescript": "5.5.4",
    "vite": "5.4.10",
    "vite-plugin-dts": "4.0.3",
    "vitest": "2.1.4"
  }
}
